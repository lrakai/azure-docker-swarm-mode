{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "workerCount": {
            "allowedValues": [
                1,
                2,
                3,
                4
            ],
            "defaultValue": 3,
            "metadata": {
                "description": "The number of workers for the cluster.  This value can be from 1 to 4"
            },
            "type": "int"
        },
        "workerEndpointDNSNamePrefix": {
            "metadata": {
                "description": "Sets the Domain name label for the worker pool IP Address.  The concatenation of the domain name label and the regional DNS zone make up the fully qualified domain name associated with the public IP address."
            },
            "defaultValue": "[concat('ca-lab-swarm-', uniqueString(resourceGroup().id, resourceGroup().name))]",
            "type": "string"
        },
        "workerSubnet": {
            "defaultValue": "10.0.0.0/16",
            "metadata": {
                "description": "Sets the subnet of worker pool 'worker'."
            },
            "type": "string"
        },
        "workerVMSize": {
            "defaultValue": "Standard_A1_v2",
            "allowedValues": [
                "Standard_A1",
                "Standard_A1_v2"
            ],
            "metadata": {
                "description": "The size of the Virtual Machine."
            },
            "type": "string"
        },
        "firstConsecutiveStaticIP": {
            "defaultValue": "172.16.0.5",
            "metadata": {
                "description": "Sets the static IP of the first manager"
            },
            "type": "string"
        },
        "linuxAdminUsername": {
            "defaultValue": "student",
            "metadata": {
                "description": "User name for the Linux Virtual Machines (SSH or Password)."
            },
            "type": "string"
        },
        "linuxAdminPassword": {
            "defaultValue": "1Cloud_Academy_Labs!",
            "type": "string"
        },
        "location": {
            "defaultValue": "[resourceGroup().location]",
            "metadata": {
                "description": "Sets the location for all resources in the cluster"
            },
            "type": "string"
        },
        "managerEndpointDNSNamePrefix": {
            "defaultValue": "[concat('ca-lab-manager-', uniqueString(resourceGroup().id, resourceGroup().name))]",
            "metadata": {
                "description": "Sets the Domain name label for the manager IP Address.  The concatenation of the domain name label and the regional DNS zone make up the fully qualified domain name associated with the public IP address."
            },
            "type": "string"
        },
        "managerSubnet": {
            "defaultValue": "172.16.0.0/24",
            "metadata": {
                "description": "Sets the subnet of the manager node(s)."
            },
            "type": "string"
        },
        "managerVMSize": {
            "defaultValue": "Standard_A1_v2",
            "allowedValues": [
                "Standard_A1",
                "Standard_A1_v2"
            ],
            "metadata": {
                "description": "The size of the Virtual Machine."
            },
            "type": "string"
        },
        "nameSuffix": {
            "defaultValue": "ca957614",
            "metadata": {
                "description": "A string hash of the manager DNS name to uniquely identify the cluster."
            },
            "type": "string"
        },
        "targetEnvironment": {
            "defaultValue": "AzureCloud",
            "metadata": {
                "description": "The azure deploy environment. Currently support: AzureCloud, AzureChinaCloud"
            },
            "type": "string"
        }
    },
    "variables": {
        "adminUsername": "[parameters('linuxAdminUsername')]",
        "adminPassword": "[parameters('linuxAdminPassword')]",
        "workerAccountName": "[concat(variables('storageAccountBaseName'), 'agnt0')]",
        "workerCount": "[parameters('workerCount')]",
        "workerCustomScript": "[concat('/usr/bin/nohup /bin/bash -c \"/bin/bash /opt/azure/containers/',variables('configureClusterScriptFile'), ' ',variables('clusterInstallParameters'),' >> /var/log/azure/cluster-bootstrap.log 2>&1 &\" &')]",
        "workerEndpointDNSNamePrefix": "[tolower(parameters('workerEndpointDNSNamePrefix'))]",
        "workerIPAddressName": "[concat(variables('orchestratorName'), '-worker-ip-', variables('workerEndpointDNSNamePrefix'), '-', variables('nameSuffix'))]",
        "workerIndex": 0,
        "workerLbBackendPoolName": "[concat(variables('orchestratorName'), '-worker-', variables('nameSuffix'))]",
        "workerLbID": "[resourceId('Microsoft.Network/loadBalancers',variables('workerLbName'))]",
        "workerLbIPConfigID": "[concat(variables('workerLbID'),'/frontendIPConfigurations/', variables('workerLbIPConfigName'))]",
        "workerLbIPConfigName": "[concat(variables('orchestratorName'), '-worker-', variables('nameSuffix'))]",
        "workerLbName": "[concat(variables('orchestratorName'), '-worker-', variables('nameSuffix'))]",
        "workerMaxVMs": 4,
        "workerRunCmd": "[concat('runcmd:\n -  [ /bin/bash, /opt/azure/containers/install-cluster.sh ]\n\n')]",
        "workerRunCmdFile": "[concat(' -  content: |\n        #!/bin/bash\n        ','sudo mkdir -p /var/log/azure\n        ',variables('workerCustomScript'),'\n    path: /opt/azure/containers/install-cluster.sh\n    permissions: \"0744\"\n')]",
        "workerStorageAccountOffset": "[mul(variables('maxStorageAccountsPerAgent'),0)]",
        "workerStorageAccountsCount": "[variables('maxStorageAccountsPerAgent')]",
        "workerSubnet": "[parameters('workerSubnet')]",
        "workerSubnetName": "[concat(variables('orchestratorName'), '-workersubnet')]",
        "workerVMNamePrefix": "[concat(variables('orchestratorName'), '-worker-', variables('nameSuffix'))]",
        "workerVMSize": "[parameters('workerVMSize')]",
        "workerVnetSubnetID": "[concat(variables('vnetID'),'/subnets/',variables('workerSubnetName'))]",
        "apiVersionDefault": "2016-03-30",
        "apiVersionStorage": "2015-06-15",
        "clusterInstallParameters": "[concat(variables('managerCount'), ' ',variables('managerVMNamePrefix'), ' ',variables('managerFirstAddrOctet4'), ' ',variables('adminUsername'),' ',variables('postInstallScriptURI'),' ',variables('managerFirstAddrPrefix'))]",
        "configureClusterScriptFile": "configure-swarmmode-cluster.sh",
        "dataStorageAccountPrefixSeed": 97,
        "location": "[variables('locations')[mod(add(2,length(parameters('location'))),add(1,length(parameters('location'))))]]",
        "locations": [
            "[resourceGroup().location]",
            "[parameters('location')]"
        ],
        "managerAvailabilitySet": "[concat(variables('orchestratorName'), '-manager-availabilitySet-', variables('nameSuffix'))]",
        "managerCount": 1,
        "managerCustomScript": "[concat('/bin/bash -c \"/bin/bash /opt/azure/containers/',variables('configureClusterScriptFile'), ' ',variables('clusterInstallParameters'),' >> /var/log/azure/cluster-bootstrap.log 2>&1\"')]",
        "managerEndpointDNSNamePrefix": "[tolower(parameters('managerEndpointDNSNamePrefix'))]",
        "managerFirstAddrOctet4": "[variables('managerFirstAddrOctets')[3]]",
        "managerFirstAddrOctets": "[split(parameters('firstConsecutiveStaticIP'),'.')]",
        "managerFirstAddrPrefix": "[concat(variables('managerFirstAddrOctets')[0],'.',variables('managerFirstAddrOctets')[1],'.',variables('managerFirstAddrOctets')[2],'.')]",
        "managerLbBackendPoolName": "[concat(variables('orchestratorName'), '-manager-pool-', variables('nameSuffix'))]",
        "managerLbID": "[resourceId('Microsoft.Network/loadBalancers',variables('managerLbName'))]",
        "managerLbIPConfigID": "[concat(variables('managerLbID'),'/frontendIPConfigurations/', variables('managerLbIPConfigName'))]",
        "managerLbIPConfigName": "[concat(variables('orchestratorName'), '-manager-lbFrontEnd-', variables('nameSuffix'))]",
        "managerLbInboundNatRules": [
            [
                {
                    "id": "[concat(variables('managerSshInboundNatRuleIdPrefix'),'0')]"
                },
                {
                    "id": "[concat(variables('managerSshPort22InboundNatRuleIdPrefix'),'0')]"
                }
            ],
            [
                {
                    "id": "[concat(variables('managerSshInboundNatRuleIdPrefix'),'1')]"
                }
            ],
            [
                {
                    "id": "[concat(variables('managerSshInboundNatRuleIdPrefix'),'2')]"
                }
            ],
            [
                {
                    "id": "[concat(variables('managerSshInboundNatRuleIdPrefix'),'3')]"
                }
            ],
            [
                {
                    "id": "[concat(variables('managerSshInboundNatRuleIdPrefix'),'4')]"
                }
            ]
        ],
        "managerLbName": "[concat(variables('orchestratorName'), '-manager-lb-', variables('nameSuffix'))]",
        "managerPublicIPAddressName": "[concat(variables('orchestratorName'), '-manager-ip-', variables('managerEndpointDNSNamePrefix'), '-', variables('nameSuffix'))]",
        "managerSshInboundNatRuleIdPrefix": "[concat(variables('managerLbID'),'/inboundNatRules/SSH-',variables('managerVMNamePrefix'))]",
        "managerSshPort22InboundNatRuleIdPrefix": "[concat(variables('managerLbID'),'/inboundNatRules/SSHPort22-',variables('managerVMNamePrefix'))]",
        "managerSshPort22InboundNatRuleNamePrefix": "[concat(variables('managerLbName'),'/SSHPort22-',variables('managerVMNamePrefix'))]",
        "managerStorageAccountName": "[concat(variables('storageAccountBaseName'), '0')]",
        "managerSubnet": "[parameters('managerSubnet')]",
        "managerSubnetName": "[concat(variables('orchestratorName'), '-managerSubnet')]",
        "managerVMNamePrefix": "[concat('ca-lab-vm-', variables('orchestratorName'), '-manager-', variables('nameSuffix'), '-')]",
        "managerVMSize": "[parameters('managerVMSize')]",
        "managerVnetSubnetID": "[concat(variables('vnetID'),'/subnets/',variables('managerSubnetName'))]",
        "maxStorageAccountsPerAgent": "[div(variables('maxVMsPerPool'),variables('maxVMsPerStorageAccount'))]",
        "maxVMsPerPool": 100,
        "maxVMsPerStorageAccount": 100,
        "nameSuffix": "[parameters('nameSuffix')]",
        "orchestratorName": "swarmm",
        "osImageOffer": "UbuntuServer",
        "osImagePublisher": "Canonical",
        "osImageSKU": "16.04-LTS",
        "osImageVersion": "16.04.201703070",
        "postInstallScriptURI": "disabled",
        "storageAccountBaseName": "[uniqueString(concat(variables('managerEndpointDNSNamePrefix'),variables('location')))]",
        "storageAccountPrefixes": [
            "0",
            "6",
            "c",
            "i",
            "o",
            "u",
            "1",
            "7",
            "d",
            "j",
            "p",
            "v",
            "2",
            "8",
            "e",
            "k",
            "q",
            "w",
            "3",
            "9",
            "f",
            "l",
            "r",
            "x",
            "4",
            "a",
            "g",
            "m",
            "s",
            "y",
            "5",
            "b",
            "h",
            "n",
            "t",
            "z"
        ],
        "storageAccountPrefixesCount": "[length(variables('storageAccountPrefixes'))]",
        "virtualNetworkName": "[concat(variables('orchestratorName'), '-vnet-', variables('nameSuffix'))]",
        "vmSizesMap": {
            "Standard_A1": {
                "storageAccountType": "Standard_LRS"
            },
            "Standard_A1_v2": {
                "storageAccountType": "Standard_LRS"
            }
        },
        "vmsPerStorageAccount": 20,
        "vnetID": "[resourceId('Microsoft.Network/virtualNetworks',variables('virtualNetworkName'))]"
    },
    "resources": [
        {
            "apiVersion": "[variables('apiVersionStorage')]",
            "copy": {
                "count": "[variables('workerStorageAccountsCount')]",
                "name": "vmLoopNode"
            },
            "dependsOn": [
                "[concat('Microsoft.Network/publicIPAddresses/', variables('managerPublicIPAddressName'))]"
            ],
            "location": "[variables('location')]",
            "name": "[concat(variables('storageAccountPrefixes')[mod(add(copyIndex(),variables('workerStorageAccountOffset')),variables('storageAccountPrefixesCount'))],variables('storageAccountPrefixes')[div(add(copyIndex(),variables('workerStorageAccountOffset')),variables('storageAccountPrefixesCount'))],variables('workerAccountName'))]",
            "properties": {
                "accountType": "[variables('vmSizesMap')[variables('workerVMSize')].storageAccountType]"
            },
            "type": "Microsoft.Storage/storageAccounts"
        },
        {
            "apiVersion": "[variables('apiVersionDefault')]",
            "location": "[variables('location')]",
            "name": "[variables('workerIPAddressName')]",
            "properties": {
                "dnsSettings": {
                    "domainNameLabel": "[variables('workerEndpointDNSNamePrefix')]"
                },
                "publicIPAllocationMethod": "Dynamic"
            },
            "type": "Microsoft.Network/publicIPAddresses"
        },
        {
            "apiVersion": "[variables('apiVersionDefault')]",
            "dependsOn": [
                "[concat('Microsoft.Network/publicIPAddresses/', variables('workerIPAddressName'))]"
            ],
            "location": "[variables('location')]",
            "name": "[variables('workerLbName')]",
            "properties": {
                "backendAddressPools": [
                    {
                        "name": "[variables('workerLbBackendPoolName')]"
                    }
                ],
                "frontendIPConfigurations": [
                    {
                        "name": "[variables('workerLbIPConfigName')]",
                        "properties": {
                            "publicIPAddress": {
                                "id": "[resourceId('Microsoft.Network/publicIPAddresses',variables('workerIPAddressName'))]"
                            }
                        }
                    }
                ],
                "inboundNatRules": [],
                "loadBalancingRules": [
                    {
                        "name": "LBRule80",
                        "properties": {
                            "backendAddressPool": {
                                "id": "[concat(variables('workerLbID'), '/backendAddressPools/', variables('workerLbBackendPoolName'))]"
                            },
                            "backendPort": 80,
                            "enableFloatingIP": false,
                            "frontendIPConfiguration": {
                                "id": "[variables('workerLbIPConfigID')]"
                            },
                            "frontendPort": 80,
                            "idleTimeoutInMinutes": 5,
                            "loadDistribution": "Default",
                            "probe": {
                                "id": "[concat(variables('workerLbID'),'/probes/tcp80Probe')]"
                            },
                            "protocol": "tcp"
                        }
                    },
                    {
                        "name": "LBRule443",
                        "properties": {
                            "backendAddressPool": {
                                "id": "[concat(variables('workerLbID'), '/backendAddressPools/', variables('workerLbBackendPoolName'))]"
                            },
                            "backendPort": 443,
                            "enableFloatingIP": false,
                            "frontendIPConfiguration": {
                                "id": "[variables('workerLbIPConfigID')]"
                            },
                            "frontendPort": 443,
                            "idleTimeoutInMinutes": 5,
                            "loadDistribution": "Default",
                            "probe": {
                                "id": "[concat(variables('workerLbID'),'/probes/tcp443Probe')]"
                            },
                            "protocol": "tcp"
                        }
                    },
                    {
                        "name": "LBRule8888",
                        "properties": {
                            "backendAddressPool": {
                                "id": "[concat(variables('workerLbID'), '/backendAddressPools/', variables('workerLbBackendPoolName'))]"
                            },
                            "backendPort": 8888,
                            "enableFloatingIP": false,
                            "frontendIPConfiguration": {
                                "id": "[variables('workerLbIPConfigID')]"
                            },
                            "frontendPort": 8888,
                            "idleTimeoutInMinutes": 5,
                            "loadDistribution": "Default",
                            "probe": {
                                "id": "[concat(variables('workerLbID'),'/probes/tcp8888Probe')]"
                            },
                            "protocol": "tcp"
                        }
                    }
                ],
                "probes": [
                    {
                        "name": "tcp80Probe",
                        "properties": {
                            "intervalInSeconds": "5",
                            "numberOfProbes": "2",
                            "port": 80,
                            "protocol": "tcp"
                        }
                    },
                    {
                        "name": "tcp443Probe",
                        "properties": {
                            "intervalInSeconds": "5",
                            "numberOfProbes": "2",
                            "port": 443,
                            "protocol": "tcp"
                        }
                    },
                    {
                        "name": "tcp8888Probe",
                        "properties": {
                            "intervalInSeconds": "5",
                            "numberOfProbes": "2",
                            "port": 8888,
                            "protocol": "tcp"
                        }
                    }
                ]
            },
            "type": "Microsoft.Network/loadBalancers"
        },
        {
            "apiVersion": "[variables('apiVersionDefault')]",
            "dependsOn": [
                "[concat('Microsoft.Network/publicIPAddresses/', variables('managerPublicIPAddressName'))]",
                "[concat('Microsoft.Storage/storageAccounts/', variables('storageAccountPrefixes')[mod(add(0,variables('workerStorageAccountOffset')),variables('storageAccountPrefixesCount'))],variables('storageAccountPrefixes')[div(add(0,variables('workerStorageAccountOffset')),variables('storageAccountPrefixesCount'))],variables('workerAccountName'))]",
                "[variables('vnetID')]",
                "[variables('workerLbID')]"
            ],
            "location": "[variables('location')]",
            "name": "[concat(variables('workerVMNamePrefix'), '-vmss')]",
            "properties": {
                "upgradePolicy": {
                    "mode": "Automatic"
                },
                "virtualMachineProfile": {
                    "networkProfile": {
                        "networkInterfaceConfigurations": [
                            {
                                "name": "nic",
                                "properties": {
                                    "ipConfigurations": [
                                        {
                                            "name": "nicipconfig",
                                            "properties": {
                                                "loadBalancerBackendAddressPools": [
                                                    {
                                                        "id": "[concat(variables('workerLbID'), '/backendAddressPools/', variables('workerLbBackendPoolName'))]"
                                                    }
                                                ],
                                                "subnet": {
                                                    "id": "[variables('workerVnetSubnetID')]"
                                                }
                                            }
                                        }
                                    ],
                                    "primary": "true"
                                }
                            }
                        ]
                    },
                    "osProfile": {
                        "adminUsername": "[variables('adminUsername')]",
                        "adminPassword": "[variables('adminPassword')]",
                        "computerNamePrefix": "[variables('workerVMNamePrefix')]",
                        "customData": "[base64(concat('#cloud-config\n\nwrite_files:\n -  encoding: gzip\n    content: !!binary |\n        \n    path: /opt/azure/containers/configure-swarmmode-cluster.sh\n    permissions: \"0744\"\n\n',variables('workerRunCmdFile'),variables('workerRunCmd')))]",
                        "linuxConfiguration": {
                            "disablePasswordAuthentication": false
                        }
                    },
                    "storageProfile": {
                        "imageReference": {
                            "offer": "[variables('osImageOffer')]",
                            "publisher": "[variables('osImagePublisher')]",
                            "sku": "[variables('osImageSKU')]",
                            "version": "latest"
                        },
                        "osDisk": {
                            "caching": "ReadWrite",
                            "createOption": "FromImage",
                            "name": "vmssosdisk",
                            "vhdContainers": [
                                "[concat(reference(concat('Microsoft.Storage/storageAccounts/',variables('storageAccountPrefixes')[mod(add(0,variables('workerStorageAccountOffset')),variables('storageAccountPrefixesCount'))],variables('storageAccountPrefixes')[div(add(0,variables('workerStorageAccountOffset')),variables('storageAccountPrefixesCount'))],variables('workerAccountName')), variables('apiVersionStorage') ).primaryEndpoints.blob, 'osdisk')]"
                            ]
                        }
                    }
                }
            },
            "sku": {
                "capacity": "[variables('workerCount')]",
                "name": "[variables('workerVMSize')]",
                "tier": "Standard"
            },
            "tags": {
                "creationSource": "[concat('acsengine-', variables('workerVMNamePrefix'), '-vmss')]"
            },
            "type": "Microsoft.Compute/virtualMachineScaleSets"
        },
        {
            "apiVersion": "[variables('apiVersionStorage')]",
            "dependsOn": [
                "[concat('Microsoft.Network/publicIPAddresses/', variables('managerPublicIPAddressName'))]"
            ],
            "location": "[variables('location')]",
            "name": "[variables('managerStorageAccountName')]",
            "properties": {
                "accountType": "[variables('vmSizesMap')[variables('managerVMSize')].storageAccountType]"
            },
            "type": "Microsoft.Storage/storageAccounts"
        },
        {
            "apiVersion": "[variables('apiVersionDefault')]",
            "location": "[variables('location')]",
            "name": "[variables('virtualNetworkName')]",
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "[variables('managerSubnet')]",
                        "[variables('workerSubnet')]"
                    ]
                },
                "subnets": [
                    {
                        "name": "[variables('managerSubnetName')]",
                        "properties": {
                            "addressPrefix": "[variables('managerSubnet')]"
                        }
                    },
                    {
                        "name": "[variables('workerSubnetName')]",
                        "properties": {
                            "addressPrefix": "[variables('workerSubnet')]"
                        }
                    }
                ]
            },
            "type": "Microsoft.Network/virtualNetworks"
        },
        {
            "apiVersion": "[variables('apiVersionDefault')]",
            "location": "[variables('location')]",
            "name": "[variables('managerAvailabilitySet')]",
            "properties": {},
            "type": "Microsoft.Compute/availabilitySets"
        },
        {
            "apiVersion": "[variables('apiVersionDefault')]",
            "location": "[variables('location')]",
            "name": "[variables('managerPublicIPAddressName')]",
            "properties": {
                "dnsSettings": {
                    "domainNameLabel": "[variables('managerEndpointDNSNamePrefix')]"
                },
                "publicIPAllocationMethod": "Dynamic"
            },
            "type": "Microsoft.Network/publicIPAddresses"
        },
        {
            "apiVersion": "[variables('apiVersionDefault')]",
            "dependsOn": [
                "[concat('Microsoft.Network/publicIPAddresses/', variables('managerPublicIPAddressName'))]"
            ],
            "location": "[variables('location')]",
            "name": "[variables('managerLbName')]",
            "properties": {
                "backendAddressPools": [
                    {
                        "name": "[variables('managerLbBackendPoolName')]"
                    }
                ],
                "frontendIPConfigurations": [
                    {
                        "name": "[variables('managerLbIPConfigName')]",
                        "properties": {
                            "publicIPAddress": {
                                "id": "[resourceId('Microsoft.Network/publicIPAddresses',variables('managerPublicIPAddressName'))]"
                            }
                        }
                    }
                ]
            },
            "type": "Microsoft.Network/loadBalancers"
        },
        {
            "apiVersion": "[variables('apiVersionDefault')]",
            "copy": {
                "count": "[variables('managerCount')]",
                "name": "managerLbLoopNode"
            },
            "dependsOn": [
                "[variables('managerLbID')]"
            ],
            "location": "[variables('location')]",
            "name": "[concat(variables('managerLbName'), '/', 'SSH-', variables('managerVMNamePrefix'), copyIndex())]",
            "properties": {
                "backendPort": 22,
                "enableFloatingIP": false,
                "frontendIPConfiguration": {
                    "id": "[variables('managerLbIPConfigID')]"
                },
                "frontendPort": "[copyIndex(22)]",
                "protocol": "tcp"
            },
            "type": "Microsoft.Network/loadBalancers/inboundNatRules"
        },
        {
            "apiVersion": "[variables('apiVersionDefault')]",
            "copy": {
                "count": "[variables('managerCount')]",
                "name": "managerLbLoopNode"
            },
            "dependsOn": [
                "[variables('managerLbID')]"
            ],
            "location": "[variables('location')]",
            "name": "[concat(variables('managerLbName'), '/', 'VIZ-', variables('managerVMNamePrefix'), copyIndex())]",
            "properties": {
                "backendPort": 8080,
                "enableFloatingIP": false,
                "frontendIPConfiguration": {
                    "id": "[variables('managerLbIPConfigID')]"
                },
                "frontendPort": "[copyIndex(8080)]",
                "protocol": "tcp"
            },
            "type": "Microsoft.Network/loadBalancers/inboundNatRules"
        },
        {
            "apiVersion": "[variables('apiVersionDefault')]",
            "dependsOn": [
                "[variables('managerLbID')]"
            ],
            "location": "[variables('location')]",
            "name": "[concat(variables('managerSshPort22InboundNatRuleNamePrefix'), '0')]",
            "properties": {
                "backendPort": 2222,
                "enableFloatingIP": false,
                "frontendIPConfiguration": {
                    "id": "[variables('managerLbIPConfigID')]"
                },
                "frontendPort": "2222",
                "protocol": "tcp"
            },
            "type": "Microsoft.Network/loadBalancers/inboundNatRules"
        },
        {
            "apiVersion": "[variables('apiVersionDefault')]",
            "copy": {
                "count": "[variables('managerCount')]",
                "name": "nicLoopNode"
            },
            "dependsOn": [
                "[variables('vnetID')]",
                "[variables('managerLbID')]",
                "[concat(variables('managerSshPort22InboundNatRuleIdPrefix'),'0')]",
                "[concat(variables('managerSshInboundNatRuleIdPrefix'),copyIndex())]"
            ],
            "location": "[variables('location')]",
            "name": "[concat(variables('managerVMNamePrefix'), 'nic-', copyIndex())]",
            "properties": {
                "ipConfigurations": [
                    {
                        "name": "ipConfigNode",
                        "properties": {
                            "loadBalancerBackendAddressPools": [
                                {
                                    "id": "[concat(variables('managerLbID'), '/backendAddressPools/', variables('managerLbBackendPoolName'))]"
                                }
                            ],
                            "loadBalancerInboundNatRules": "[variables('managerLbInboundNatRules')[copyIndex()]]",
                            "privateIPAddress": "[concat(variables('managerFirstAddrPrefix'), copyIndex(int(variables('managerFirstAddrOctet4'))))]",
                            "privateIPAllocationMethod": "Static",
                            "subnet": {
                                "id": "[variables('managerVnetSubnetID')]"
                            }
                        }
                    }
                ]
            },
            "type": "Microsoft.Network/networkInterfaces"
        },
        {
            "apiVersion": "[variables('apiVersionDefault')]",
            "copy": {
                "count": "[variables('managerCount')]",
                "name": "vmLoopNode"
            },
            "dependsOn": [
                "[concat('Microsoft.Network/networkInterfaces/', variables('managerVMNamePrefix'), 'nic-', copyIndex())]",
                "[concat('Microsoft.Compute/availabilitySets/',variables('managerAvailabilitySet'))]",
                "[variables('managerStorageAccountName')]"
            ],
            "location": "[variables('location')]",
            "name": "[concat(variables('managerVMNamePrefix'), copyIndex())]",
            "properties": {
                "availabilitySet": {
                    "id": "[resourceId('Microsoft.Compute/availabilitySets',variables('managerAvailabilitySet'))]"
                },
                "hardwareProfile": {
                    "vmSize": "[variables('managerVMSize')]"
                },
                "networkProfile": {
                    "networkInterfaces": [
                        {
                            "id": "[resourceId('Microsoft.Network/networkInterfaces',concat(variables('managerVMNamePrefix'), 'nic-', copyIndex()))]"
                        }
                    ]
                },
                "osProfile": {
                    "adminUsername": "[variables('adminUsername')]",
                    "adminPassword": "[variables('adminPassword')]",
                    "computerName": "[concat(variables('managerVMNamePrefix'), copyIndex())]",
                    "customData": "[base64('#cloud-config\n\nwrite_files:\n -  encoding: gzip\n    content: !!binary |\n        \n    path: /opt/azure/containers/configure-swarmmode-cluster.sh\n    permissions: \"0744\"\n\n')]",
                    "linuxConfiguration": {
                        "disablePasswordAuthentication": false
                    }
                },
                "storageProfile": {
                    "imageReference": {
                        "offer": "[variables('osImageOffer')]",
                        "publisher": "[variables('osImagePublisher')]",
                        "sku": "[variables('osImageSKU')]",
                        "version": "[variables('osImageVersion')]"
                    },
                    "osDisk": {
                        "caching": "ReadWrite",
                        "createOption": "FromImage",
                        "name": "[concat(variables('managerVMNamePrefix'), copyIndex(),'-osdisk')]",
                        "vhd": {
                            "uri": "[concat(reference(concat('Microsoft.Storage/storageAccounts/', variables('managerStorageAccountName')), variables('apiVersionStorage')).primaryEndpoints.blob, 'vhds/', variables('managerVMNamePrefix'), copyIndex(), '-osdisk.vhd')]"
                        }
                    }
                }
            },
            "tags": {
                "creationSource": "[concat('acsengine-', variables('managerVMNamePrefix'), copyIndex())]"
            },
            "type": "Microsoft.Compute/virtualMachines"
        },
        {
            "apiVersion": "[variables('apiVersionDefault')]",
            "copy": {
                "count": "[variables('managerCount')]",
                "name": "vmLoopNode"
            },
            "dependsOn": [
                "[concat('Microsoft.Compute/virtualMachines/', concat(variables('managerVMNamePrefix'), copyIndex()))]"
            ],
            "location": "[variables('location')]",
            "name": "[concat(variables('managerVMNamePrefix'), copyIndex(), '/configuremanager')]",
            "properties": {
                "publisher": "Microsoft.OSTCExtensions",
                "settings": {
                    "commandToExecute": "[variables('managerCustomScript')]",
                    "fileUris": []
                },
                "type": "CustomScriptForLinux",
                "typeHandlerVersion": "1.4"
            },
            "type": "Microsoft.Compute/virtualMachines/extensions"
        }
    ],
    "outputs": {
        "workerFQDN": {
            "type": "string",
            "value": "[reference(concat('Microsoft.Network/publicIPAddresses/', variables('workerIPAddressName'))).dnsSettings.fqdn]"
        },
        "managerFQDN": {
            "type": "string",
            "value": "[reference(concat('Microsoft.Network/publicIPAddresses/', variables('managerPublicIPAddressName'))).dnsSettings.fqdn]"
        }
    }
}